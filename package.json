{
  "name": "koa-simple-proxy-middleware",
  "version": "0.2.13",
  "description": "http proxy middleware for koa",
  "engines": {
    "node": ">=10.0.0"
  },
  "engineStrict": true,
  "main": "index.js",
  "scripts": {
    "test": "npm -s run mocha && npm run -s lint && npm run -s jscs",
    "test:debug": "mocha debug -R spec test --recursive",
    "mocha": "mocha -R spec test --recursive",
    "lint": "jshint index.js test/*.js test/**/*js lib/*js app/**/*js ",
    "jscs": "jscs index.js test/*.js test/**/*js lib/*js app/**/*js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/instabot42/koa-simple-proxy-middleware.git"
  },
  "types": "types.d.ts",
  "keywords": [
    "koa-better-http-proxy"
  ],
  "author": {
    "name": "instabot",
    "email": "instabot@example.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/instabot42/koa-simple-proxy-middleware/issues"
  },
  "devDependencies": {
    "body-parser": "^1.17.1",
    "chai": "^4.5.0",
    "cookie-parser": "^1.4.6",
    "express": "^4.19.2",
    "koa": "^2.15.3",
    "mocha": "^10.7.3",
    "supertest": "^7.0.0"
  },
  "dependencies": {
    "es6-promise": "^4.2.8",
    "raw-body": "^3.0.0"
  },
  "contributors": []
}
